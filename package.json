{
  "name": "graweria",
  "version": "1.0.0",
  "scripts": {
    "server": "netlify-cms-proxy-server",
    "quicktype": "quicktype public/cms/content -o ./src/types/content.ts --src-lang json --lang typescript --just-types && prettier ./src/types/content.ts --write",
    "dev": "npm run process-cms-blog && concurrently --names \"CMS-PROXY,NEXT\" -c \"blue.bold,magenta.bold\" \"npm:server\" \"next\"",
    "build": "npm run process-cms-blog && next build",
    "start": "next start",
    "analyze": "ANALYZE=true npm run build",
    "process-cms-blog": "node process.cms.blog.js"
  },
